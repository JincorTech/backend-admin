{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.6.4",
        "alcaeus/mongo-php-adapter": "^1.1",
        "barryvdh/laravel-ide-helper": "^2.3",
        "doctrine/dbal": "~2.3",
        "doctrine/mongodb": "~1.3",
        "doctrine/mongodb-odm": "1.1.1",
        "gedmo/doctrine-extensions": "^3.0@dev",
        "guzzlehttp/guzzle": "^6.3",
        "infyomlabs/adminlte-templates": "5.4.x-dev",
        "infyomlabs/laravel-generator": "5.4.x-dev",
        "infyomlabs/swagger-generator": "dev-master",
        "jenssegers/mongodb": "^3.1",
        "jincor/laravel-auth-client": "dev-master",
        "jlapp/swaggervel": "dev-master",
        "laravel/framework": "5.4.23",
        "laravel/tinker": "~1.0",
        "laravelcollective/html": "^5.4.0",
        "pimlie/laravel-datatables-mongodb": "dev-master",
        "predis/predis": "^2.0@dev",
        "yajra/laravel-datatables-buttons": "^1.0",
        "yajra/laravel-datatables-oracle": "^7.2"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.7"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.local', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "provide": {
      "ext-mongo": "1.6.14"
    }
}
